{
    "name": "Recent Bookmarks",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "A list of recent bookmarks in descending order, grouped by the day they were added.Edited from old Recent Bookmarks in chrome store",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_icon": "icons/icon48.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "bookmarks",
        "chrome://favicon/"
    ]
}